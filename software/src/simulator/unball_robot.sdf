<?xml version='1.0'?>
<sdf version='1.4'>
  <model name="unball_robot">

  <static>false</static>

  <link name='chassis'>
    <pose>0 0 .1 0 0 0</pose>
    <collision name='collision'>
      <geometry>
        <box>
          <size>.075 .075 .075</size>
        </box>
      </geometry>
    </collision>
    <visual name='visual'>
      <geometry>
        <box>
          <size>.075 .075 .075</size>
        </box>
      </geometry>
    </visual>
  </link>     
  
  <link name="left_wheel">
    <pose>0 0.0375 0.08 0 1.5707 1.5707</pose>
    <collision name="collision">
      <geometry>
        <cylinder>
          <radius>.0375</radius>
          <length>.01</length>
        </cylinder>
      </geometry>
    </collision>
    <visual name="visual">
      <geometry>
        <cylinder>
          <radius>.0375</radius>
          <length>.01</length>
        </cylinder>
      </geometry>
    </visual>
  </link>

  <link name="right_wheel">
    <pose>0 -0.0375 0.08 0 1.5707 1.5707</pose>
    <collision name="collision">
      <geometry>
        <cylinder>
          <radius>.0375</radius>
          <length>.01</length>
        </cylinder>
      </geometry>
    </collision>
    <visual name="visual">
      <geometry>
        <cylinder>
          <radius>.0375</radius>
          <length>.01</length>
        </cylinder>
      </geometry>
    </visual>
  </link>

  <joint type="revolute" name="left_wheel_hinge">
    <pose>0 0 -0.03 0 0 0</pose>
    <child>left_wheel</child>
    <parent>chassis</parent>
    <axis>
      <xyz>0 1 0</xyz>
    </axis>
  </joint>

  <joint type="revolute" name="right_wheel_hinge">
    <pose>0 0 0.03 0 0 0</pose>
    <child>right_wheel</child>
    <parent>chassis</parent>
    <axis>
      <xyz>0 1 0</xyz>
    </axis>
  </joint>

  </model>
</sdf>
